{
  "name": "promise-singleflight",
  "version": "0.0.23",
  "description": "provide a wrap async function for singleflight",
  "scripts": {
    "clean": "rm -r dist",
    "compile": "npm run clean;npm run build:cjs;npm run build:esm;npm run build:types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "ncu": "npx npm-check-updates",
    "test": "jest --coverage"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "author": "Joey Fang <josudoey@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/esm": "^1.0.0",
    "@tsconfig/node16": "^1.0.2",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.0",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josudoey/promise-singleflight.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "promise",
    "singleflight",
    "async"
  ],
  "bugs": {
    "url": "https://github.com/josudoey/promise-singleflight/issues"
  },
  "homepage": "https://github.com/josudoey/promise-singleflight#readme"
}